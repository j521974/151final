import java.awt.Graphics;

public class DOval extends DShape {

	public DOval(DShapeModel model) {
		super(model);
		// TODO Auto-generated constructor stub
	}
	//draw method
	public void draw(Graphics x){
		x.setColor(model.getColor());
		x.fillOval(model.getX(), model.getY(), model.getWidth(), model.getHeight());
	}
}
